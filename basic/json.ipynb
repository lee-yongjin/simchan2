   "source": [
    "import json\n",
    "\n",
    "# Python → JSON 문자열\n",
    "data = {\"name\": \"홍길동\", \"age\": 30}\n",
    "json_str = json.dumps(data)\n",
    "\n",
    "# JSON 문자열 → Python 객체\n",
    "parsed = json.loads(json_str)\n",
    "print(parsed[\"name\"])  # \"홍길동\"\n",
    "print(parsed[\"age\"]) \n",
    "\n",
    "for key, value in parsed.items():\n",
    "    print(f\"{key}: {value}\")\n"
   ]
  }
 ],
